{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/scripts/adminSeeder.mjs", "src/app/api/admin/contact/[id]/route.js", "src/app/api/admin/orders/[orderId]/route.js", "src/app/api/orders/[orderId]/route.js", "src/app/api/orders/receipt/[id]/route.js", "src/app/api/reviews/[id]/route.js", "src/app/api/user/products/[id]/route.js", "src/app/api/user/review/[productId]/route.js"],
  "exclude": ["node_modules"]
}
